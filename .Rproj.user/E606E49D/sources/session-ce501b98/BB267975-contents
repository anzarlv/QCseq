# Protein sequences
install.packages("protr")
library("protr")
# load FASTA files
extracell <- protr::readFASTA(
  file = system.file("protseq/extracell.fasta", package = "protr"))
length(extracell) # 325
extracell$P47710

mitonchon <- protr::readFASTA(
  file = system.file("protseq/mitochondrion.fasta", package = "protr"))
length(mitonchon) # 306
mitonchon$Q16762



#### Example BED file ####

# install.packages("GenomicTools.fileHandler")
library(GenomicTools.fileHandler)
# More info: https://cran.rstudio.com/web/packages/GenomicTools.fileHandler/index.html
ls("package:GenomicTools.fileHandler")
# Define here the location on HDD for the example file
fpath <- system.file("extdata",
                     "example.bed", 
                     package = "GenomicTools.fileHandler")
# Import the example bed file
bedFile <- GenomicTools.fileHandler::importBed(file = fpath)
dim(bedFile) # 9 3
names(bedFile)



#### Example FASTA file ####

# install.packages("GenomicTools.fileHandler")
library(GenomicTools.fileHandler)
# More info: https://cran.rstudio.com/web/packages/GenomicTools.fileHandler/index.html
ls("package:GenomicTools.fileHandler")

# Define here the location on HDD for the example file
fpath <- system.file("extdata", 
                     "example.fasta", 
                     package = "GenomicTools.fileHandler")
# Import the example fasta file  
fastaFile <- GenomicTools.fileHandler::importFA(file = fpath)
length(fastaFile) # 1000
names(fastaFile)



#### Example FASTQ file ####

# install.packages("GenomicTools.fileHandler")
library(GenomicTools.fileHandler)
# More info: https://cran.rstudio.com/web/packages/GenomicTools.fileHandler/index.html
ls("package:GenomicTools.fileHandler")
# Define here the location on HDD for the example file
fpath <- system.file("extdata",
                     "example.fastq", 
                     package = "GenomicTools.fileHandler")
# Import the example fastq file  
fastqFile <- GenomicTools.fileHandler::importFQ(file = fpath)
length(fastqFile) # 2
names(fastqFile$seq)
names(fastqFile$qual)



