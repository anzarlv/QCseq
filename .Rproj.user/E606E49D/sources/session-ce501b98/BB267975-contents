# Load packages for Bioconductor or GitHub
# install.packages("BiocManager") # before downloading Bioconductor packages
library(BiocManager)
# For more datasets, visit:
# https://www.bioconductor.org/packages/release/BiocViews.html#___ExperimentData

# install.packages("devtools") # before downloading GitHub packages
library(devtools)



####  Annotation Tables #### 
devtools::install_github("stephenturner/annotables")
library(annotables)
data(package = "annotables") # has many datasets
dim(grch37) # Example; 66978     9
class(grch37) 


#### Single-Cell RNA-Seq ####

# Public Single-Cell RNA-Seq Datasets
BiocManager::install("scRNAseq")
library(scRNAseq)
data(package = "scRNAseq") # no data sets found
ls("package:scRNAseq") # to see datasets
# see scRNAseq Bioconductor page for obtaining and using data
scRichardTCellData <- scRNAseq::RichardTCellData()
dim(scRichardTCellData) # Example;  46603   572
slotNames(scRichardTCellData)
class(scRichardTCellData) # "SingleCellExperiment"



BiocManager::install("scruff")
library(scruff)
ls("package:scruff")
data(sceExample)
slotNames(sceExample)
class(sceExample) # "SingleCellExperiment"



#### RNA-seq Datasets ####

# RNA-seq datasets from The Cancer Genome Atlas Project
BiocManager::install("RTCGA.rnaseq")
library(RTCGA.rnaseq)
data(package = "RTCGA.rnaseq") # shows datasets
# see RTCGA.rnaseq Bioconductor page for obtaining and using data
dim(ACC.rnaseq) # Example; 79 20532



# Sample RNAseq raw counts dataset
install.packages("MBCluster.Seq")
library("MBCluster.Seq")
data("Count")
dim(Count) # 1000    8
?Count # For 1000 genes, 4 treatment and 2 replicates



# Simulated RNAseq count data
devtools::install_github("anjalisilva/MPLNClust", build_vignettes = TRUE)
library("MPLNClust")
ls("package:MPLNClust")
?mplnDataGenerator
# Generating simulated data
trueMu1 <- c(6.5, 6, 6, 6, 6, 6)
trueMu2 <- c(2, 2.5, 2, 2, 2, 2)
trueSigma1 <- diag(6) * 2
trueSigma2 <- diag(6)
sampleData <- MPLNClust::mplnDataGenerator(nObservations = 100,
                                           dimensionality = 6,
                                           mixingProportions = c(0.79, 0.21),
                                           mu = rbind(trueMu1, trueMu2),
                                           sigma = rbind(trueSigma1, trueSigma2),
                                           ImageName = "TwoComponents")
dim(sampleData$dataset) # 100   6



