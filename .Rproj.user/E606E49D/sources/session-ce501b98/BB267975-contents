#### Example PED/MAP file (for linkage pedigree data) ####

# install.packages("GenomicTools.fileHandler")
library(GenomicTools.fileHandler)
# More info: https://cran.rstudio.com/web/packages/GenomicTools.fileHandler/index.html
ls("package:GenomicTools.fileHandler")
# Define here the location on HDD for the example file
pedPath <- system.file("extdata",
                       "example.ped", 
                       package = "GenomicTools.fileHandler")
mapPath <- system.file("extdata",
                       "example.map", 
                       package = "GenomicTools.fileHandler")
# Import the example ped/map files
pedmapFile <- GenomicTools.fileHandler::importPED(file = pedPath, 
                                                  snps = mapPath)
length(pedmapFile) # 4
names(pedmapFile)
pedmapFile$genotypes


