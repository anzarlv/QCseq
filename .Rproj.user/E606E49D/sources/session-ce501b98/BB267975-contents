#### Example GTF file ####

# install.packages("GenomicTools.fileHandler")
library(GenomicTools.fileHandler)
# More info: https://cran.rstudio.com/web/packages/GenomicTools.fileHandler/index.html
ls("package:GenomicTools.fileHandler")
# Define here the location on HDD for the example file
fpath <- system.file("extdata",
                     "example.gtf", 
                     package = "GenomicTools.fileHandler")
# Import the example gtf file
GTFFile <- GenomicTools.fileHandler::importGTF(fpath, 
                                               level = "transcript",
                                               features = c("gene_id", "FPKM"))
dim(GTFFile) # 158  10
head(GTFFile)



